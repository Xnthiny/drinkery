{
  "name": "drinkery",
  "version": "1.0.0",
  "description": "UM Coding Bootcamp Project 3",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^1.3.2",
    "helmet": "^3.18.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-maps": "^9.4.5",
    "react-modal-video": "^1.2.2",
    "react-player": "^1.11.0",
    "redux": "^4.0.1",
    "validator": "^10.11.0",
    "video-react": "^0.13.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xnthiny/drinkery.git"
  },
  "bugs": {
    "url": "https://github.com/Xnthiny/drinkery/issues"
  },
  "homepage": "https://github.com/Xnthiny/drinkery#readme"
}
